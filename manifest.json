{
  "manifest_version": 2,

  "name": "Website Reactions",
  "description": "This extension allows you to react and see the current reactions of a Website.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png", 
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "permissions": [
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs"
    ]

}
